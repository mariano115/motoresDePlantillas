<% if(productos.length !== 0) { %>
    <div class="table-responsive" style="background-color: rgb(160, 160, 204); width: 50%; display: flex ; justify-content: center;">
        <table class="table table-dark">
            <tr style="color: rgb(0, 0, 0);"> <th style="color: white">Nombre</th> <th style="color: white">Precio</th> <th style="color: white">Foto</th> </tr>
           <% for (const producto in productos ) { %>
            <tr>
                <td style="color: white"> <%= productos[producto].nombre %> </td>
                <td style="color: white"> <%= productos[producto].precio %> </td>
                <td style="color: white"> <img style="width: 50px;" src= <%= productos[producto].foto %>> </td>
            </tr>
            <% } %>
        </table>
    </div>
<% } else{ %> 
   <h3 style="color: black">no existen datos</h3>
<% }%>

